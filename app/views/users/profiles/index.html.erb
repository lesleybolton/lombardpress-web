<div class="container">
	<h1>Users</h1>
	<table class="table table-striped table-hover">
		<thead>
			<tr>
				<td>Email</td>
				<td>Role</td>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
				<tr>
					<td><%= user.email %></td>
					<td><%= user.role %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>
		