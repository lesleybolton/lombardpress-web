<div class="container">
	<h1>Users</h1>
	<table class="table table-striped table-hover">
		<thead>
			<tr>
				<td>Email</td>
				<td>Role</td>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
				<tr>
					<td><%= link_to user.email, "/users/profiles/" + user.id.to_s %></td>
					<td><%= user.role %></td>
					<td><%= user.created_at %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>
		