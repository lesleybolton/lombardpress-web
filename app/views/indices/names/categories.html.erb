<div class="container lbp-index-container">
	<h1>Name Categories</h1>
		<div class="lbp-index-breadcrumbs">
			<%= link_to "Back", :back %>
			<%= link_to "Return to Index", "/index" %>
		</div>
	
	<table class="table table-striped table-hover">
		<% @categories.each do |category| %>
			<tr>
				<td><%= link_to category, "/index/names/list/#{category}" %></td>
			</tr>
		<% end %>	
	</table>
</div>